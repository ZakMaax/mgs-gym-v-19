<?xml version="1.0" encoding="utf-8"?>
<odoo>

      <record id="mgs_gym_membership_report_wizard_form_view" model="ir.ui.view">
        <field name="name">mgs_gym.membership_report_wizard_form_view</field>
        <field name="model">mgs_gym.membership_report_wizard</field>
        <field name="arch" type="xml">
            <form string="Membership Report">
                <sheet>
                    <group>
                        <group>
                            <field name="branch_id"/>
                            <field name="shift_id"/>
                        </group>
                        <group>
                            <field name="recurrence_unit"/>
                            <field name="state_id"/>
                        </group>
                    </group>
                     <footer>
                        <button class="btn btn-primary" string="Print" type="object" name="action_print_report" />
                        <button class="btn btn-success" string="Print Excel" type="object" name="action_generate_excel" />
                        <button class="btn btn-danger" string="Cancel"  special="cancel" />
                    </footer>
                </sheet>
            </form>

        </field>
    </record>

</odoo>
